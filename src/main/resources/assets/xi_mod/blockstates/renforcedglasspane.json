{
  "multipart": [
    {
      "apply": {
        "model": "xi_mod:block/renforcedglasspane_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "xi_mod:block/renforcedglasspane_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "xi_mod:block/renforcedglasspane_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "xi_mod:block/renforcedglasspane_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "xi_mod:block/renforcedglasspane_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "xi_mod:block/renforcedglasspane_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "xi_mod:block/renforcedglasspane_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "xi_mod:block/renforcedglasspane_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "xi_mod:block/renforcedglasspane_noside",
        "y": 270
      }
    }
  ]
}